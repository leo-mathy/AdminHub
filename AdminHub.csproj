<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Ressources\CloseIcon.png" />
    <None Remove="Ressources\MinimizeIcon.png" />
    <None Remove="Ressources\PasswordIcon.png" />
    <None Remove="Ressources\PersonIcon.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Ressources\CloseIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Ressources\MinimizeIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Resource Include="Ressources\PasswordIcon.png" />
    <Content Include="Ressources\PersonIcon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
